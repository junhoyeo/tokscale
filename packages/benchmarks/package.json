{
  "name": "@tokscale/benchmarks",
  "version": "0.1.0",
  "private": true,
  "description": "Benchmark scripts for tokscale CLI",
  "type": "module",
  "scripts": {
    "generate": "tsx generate.ts",
    "run": "tsx runner.ts",
    "run:synthetic": "tsx runner.ts --synthetic --iterations 5 --warmup 1",
    "run:rust": "tsx runner.ts --implementation rust",
    "run:rust:synthetic": "tsx runner.ts --implementation rust --synthetic --iterations 5 --warmup 1"
  }
}
